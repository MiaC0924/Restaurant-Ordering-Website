<h1>Profile for {{username}}</h1>

<div>
  <div style="float:left;">
    <table>
      <tr>
        <td>
          <h4>{{username}}'s Cards</h4>
          {{#each cards}}
              <a  href="/card/{{this._id}}">{{this.name}}</a><br>
          {{/each}}
        </td>
      </tr>
    </table>
  </div>